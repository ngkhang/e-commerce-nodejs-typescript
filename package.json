{
  "name": "e-commerce-nodejs-typescript",
  "version": "1.0.0",
  "author": "Khang Nguyen",
  "license": "ISC",
  "description": "An e-commerce application built with Node.js and TypeScript",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "start": "node ./dist/server.js",
    "dev": "tsx watch ./src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "inquirer": "^9.3.8",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "homepage": "https://github.com/ngkhang/e-commerce-nodejs-typescript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngkhang/e-commerce-nodejs-typescript.git"
  },
  "keywords": [
    "nodejs",
    "typescript"
  ]
}
